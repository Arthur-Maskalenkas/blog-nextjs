{
  "name": "Blog-Nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build  && yarn postbuild",
    "postbuild": "next-sitemap --config next-generate-sitemap.js",
    "generateAlgoliaSearch": "node ./scripts/build-search.js",
    "start": "next start",
    "analyze": "next build",
    "lint": "eslint src --max-warnings=0",
    "generate": "yarn plop --plopfile ./generators/atomsGenerator.js",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "eslint:fix": "eslint src/pages src/components --fix"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@types/react-instantsearch-dom": "^6.12.2",
    "algoliasearch": "^4.12.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gray-matter": "^4.0.3",
    "gtag": "^1.0.1",
    "next": "12.3.1",
    "next-pwa": "^5.3.1",
    "next-seo": "^5.1.0",
    "nextjs-progressbar": "^0.0.14",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-instantsearch-dom": "^6.22.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-prism-plus": "^1.3.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-rehype": "^10.1.0",
    "slugify": "^1.6.5",
    "styled-components": "5.3.5",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.22.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.22",
    "plop": "^3.0.5",
    "typescript": "^4.5.5",
    "webpack": "5.74.0"
  }
}
